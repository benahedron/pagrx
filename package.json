{
  "name": "pagrx",
  "version": "0.1.0",
  "description": "A reactive page based REST API access that allows nesting. Based on RxJS.",
  "main": "index.js",
  "scripts": {
    "test": "ts-mocha 'test/**/*.spec.ts'",
    "test:watch": "ts-mocha 'test/**/*.spec.ts' -w --watch-files src/**/*,test/**/* ",
    "test:e2e": "ts-node samples/simple-paged-users.ts",
    "test:e2e-window:watch": "ts-node samples/simple-window.ts -w --watch-files src/**/*,test/**/* ",
    "test:watch-window": "ts-mocha 'test/**/*-window.spec.ts' -w --watch-files src/**/*,test/**/* ",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "keywords": [
    "page",
    "cache",
    "proxy",
    "api",
    "rest"
  ],
  "author": "Bernard Reichert",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.13",
    "chai": "^4.3.7",
    "prettier": "^2.8.7",
    "sinon": "^15.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.2"
  },
  "files": [
    "lib/**/*"
  ]
}
